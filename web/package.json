{
  "name": "my-dockers",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "prebuild": "npx tsc --jsx preserve --module nodenext --target es2020 --allowSyntheticDefaultImports --esModuleInterop --outdir temp --inlineSourceMap --inlineSources src/*.ts src/*.tsx",
    "build": "node scripts/build-web.cjs",
    "preserv": "npm run prebuild",
    "serv": "concurrently \"node scripts/build-web.cjs s\" \"npm run prebuild -- --watch\""
  },
