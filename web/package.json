    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/react-router": "^5.1.20",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.1",
    "esbuild-copy-static-files": "^0.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
